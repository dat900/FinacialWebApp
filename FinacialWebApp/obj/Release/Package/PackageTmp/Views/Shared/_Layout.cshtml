<body>
	<div class="wrapper ">
		<div class="sidebar" data-color="blue">
			<!--
				Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
			-->
			<div class="logo">
				<!--a href="http://www.creative-tim.com" class="simple-text logo-mini">
					CT
				</a-->
				<a href="../Incomes/Incomes" class="simple-text logo-normal">
					Analysis View
				</a>
			</div>
			<div class="sidebar-wrapper">
				<ul class="nav">
					<li class="menu-item" id="dashboard">
						<a href="../Incomes/Incomes">
							<i class="now-ui-icons business_bank"></i>
							<p>Incomes</p>
						</a>
					</li>

					<li class="menu-item" id="product">
						<a href="../Outcomes/Outcomes">
							<i class="now-ui-icons business_money-coins"></i>
							<p>OutComes</p>
						</a>
					</li>

					<li class="menu-item" id="top_brand">
						<a href="../Estimation/Index">
							<i class="now-ui-icons business_chart-bar-32"></i>
							<p>Estimation</p>
						</a>
					</li>

					<li class="menu-item" id="customer">
						<a href="../EditIncomes/Index">
							<i class="now-ui-icons design_bullet-list-67"></i>
							<p>Edit Incomes</p>
						</a>
					</li>

					<li class="menu-item" id="sales_detail">
						<a href="../EditOutcomes/Index">
							<i class="now-ui-icons design_bullet-list-67"></i>
							<p>Edit Outcomes</p>
						</a>
					</li>

					@*<li class="" id="report_cus">
						<a href="../Home/Cus_Report">
							<i class="now-ui-icons education_paper"></i>
							<p>Customer Report</p>
						</a>
					</li>

					<li class="" id="report_watches">
						<a href="../Home/Watches_Report">
							<i class="now-ui-icons education_atom"></i>
							<p>Watches Report</p>
						</a>
					</li>*@

				</ul>
			</div>
		</div>
		<div class="main-panel">
			@RenderBody()			
			@RenderSection("scripts", required: false)
		</div>
	</div>


</body>
</html>